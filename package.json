{
  "name": "docubrain",
  "version": "1.0.0",
  "description": "DocuBrain - AI-powered document intelligence platform",
  "main": "index.js",
  "scripts": {
    "install-frontend": "cd frontend && yarn install",
    "install-backend": "cd backend && pip install -r requirements.txt",
    "build-frontend": "cd frontend && yarn build",
    "start-frontend": "cd frontend && yarn start",
    "start-backend": "cd backend && uvicorn server:app --host 0.0.0.0 --port 8000",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "keywords": ["ai", "documents", "rag", "fastapi", "react"],
  "author": "DocuBrain Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}